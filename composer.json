{
    "name": "elshiha/laravel-sockeon",
    "description": "Laravel integration for Sockeon WebSocket server with artisan commands",
    "keywords": [
        "laravel",
        "websocket",
        "sockeon",
        "real-time",
        "websockets"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahmoud Elshiha",
            "email": "MahmoudElshiha@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/console": ">=11.0",
        "illuminate/support": ">=11.0",
        "sockeon/sockeon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Elshiha\\LaravelSockeon\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elshiha\\LaravelSockeon\\SockeonServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}