{
    "name": "sockeon/laravel-sockeon",
    "description": "Laravel integration for Sockeon WebSocket server with artisan commands",
    "keywords": [
        "laravel",
        "websocket",
        "sockeon",
        "real-time",
        "websockets"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahmoud Elshiha",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "sockeon/sockeon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Sockeon\\LaravelSockeon\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sockeon\\LaravelSockeon\\SockeonServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}